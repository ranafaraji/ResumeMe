@model Resume_Portfolio.Models.Information
@{
    ViewBag.Title = "Print";
}

<h2>@Model.firstName</h2>



@*<div class="tabContent" style="display: none;" id="finish">*@
    <div class="container-fluid">
        <div class="row">
            <div class="col-12 col-xl-12 col-sm-12 col-md-12 text-center">
                <h2 id="nameFamily" class="styles"></h2>
            </div>
        </div>
        <div class="row">
          
            <div class="col-12 col-sm-12 col-md-2 col-lg-2 col-xl-2">
                <p id="phone"> <strong><i>Phone : @Model.phoneNumber</i></strong> </p>
            </div>
            <div class="col-12 col-sm-12 col-md-2 col-lg-2 col-xl-2">
                <p id="country"><strong><i>Country : @Model.country</i></strong></p>
            </div>
            <div class="col-12 col-sm-12 col-md-2 col-lg-2 col-xl-2">
                <p id="passport"><strong><i>Passport :  @Model.passportNumber</i></strong></p>
            </div>
            <div class="col-12 col-sm-12 col-md-2 col-lg-2 col-xl-2">
                <p id="address"><strong><i>Address :  @Model.address</i></strong></p>
            </div>
            <div class="col-12 col-sm-12 col-md-2 col-lg-3 col-xl-3">
                <p id="email"><strong><i>Email : @Model.email</i></strong></p>
            </div>

        </div>

        <div class="row mt-3">
            <div class="col-12 col-xl-12 col-sm-12 col-md-12 text-center">
                <h1 class="styles">Summary</h1>

            </div>
        </div>
        <div class="row mt-1">
            <div class="col-12 col-xl-12 col-sm-12 col-md-12 text-center">
                <p id="summaryReport">@Model.summary</p>
            </div>
        </div>


        <div class="row mt-3">
            <div class="col-12 col-xl-12 col-sm-12 col-md-12 text-center">
                <h1 class="styles">Education </h1>

            </div>
        </div>
        <div class="row">
            <div class="row">
                <div class="col-12 col-sm-12 col-md-2 col-lg-2 col-xl-2">
                    <span id="school"><strong><i>School : @Model.school</i></strong> </span>
                </div>
                <div class="col-12 col-sm-12 col-md-2 col-lg-2 col-xl-2">
                    <span id="degree"><strong><i>Degree : @Model.degree</i></strong></span>
                </div>
                <div class="col-12 col-sm-12 col-md-2 col-lg-2 col-xl-2">
                    <span id="date"><strong><i>Date : @Model.dateEducation</i></strong></span>
                </div>
                <div class="col-12 col-sm-12 col-md-2 col-lg-2 col-xl-2">
                    <span id="city"><strong><i>City : @Model.cityEducation</i></strong></span>
                </div>
                <div class="row mt-3">
                    <div class="col-12 col-sm-12 col-xl-12 col-md-12 col-lg-12">
                        <span id="description"><strong><i>Description : @Model.descriptionEducation</i></strong></span>
                    </div>
                </div>
            </div>




            <div class="row mt-3">
                <div class="col-12 col-xl-12 col-sm-12 col-md-12 text-center">
                    <h1 class="styles"> Experience</h1>

                </div>
                <div class="row">
                    <div class="col-12 col-sm-12 col-md-12 col-lg-12 col-xl-12">
                        <span id="job"><strong><i>Job title: @Model.jobTitle</i></strong></span>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12 col-sm-12 col-md-12 col-lg-12 col-xl-12">
                        <span id="employee"><strong><i>Employee : @Model.Employee</i></strong></span>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12 col-sm-12 col-md-12 col-lg-12 col-xl-12">
                        <span id="startDate"><strong><i>Start & End Date: @Model.dateWork</i></strong> </span>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12 col-sm-12 col-md-12 col-lg-12 col-xl-12">
                        <span id="CityAgain"><strong><i>City : @Model.cityWork</i></strong></span>
                    </div>
                </div>
                <div class="row mt-3">
                    <div class="col-12 col-sm-12 col-xl-12 col-md-12 col-lg-12">
                        <span id="DesAgain"><strong><i>Description : @Model.descriptionWork</i></strong></span>
                    </div>
                </div>
            </div>


        </div>


        <div class="row mt-3">
            <div class="col-12 col-xl-12 col-sm-12 col-md-12 text-center">
                <h1 class="styles"> Skills </h1>

            </div>
        </div>
        <div class="row mt-1">

            <div class="col-12 col-sm-12 col-xl-12 col-md-12 col-lg-12">
                <div>

                    <ul>
                        <li id="basic"><strong><i> Basic : @Model.basicSkill</i></strong></li>
                        <li id="intermadiate"><strong><i>Intermediate : @Model.intermediateSkill</i></strong></li>
                        <li id="advance"><strong><i> Advance : @Model.advanceSkill</i></strong></li>
                    </ul>
                </div>
            </div>
        </div>


        <div class="row mt-3">
            <div class=" col-12 col-xl-12 col-sm-12 col-lg-12 col-md-12 text-center">
                <h1 class="styles"> Reference </h1>
                <hr>
                <div class="row">
                    <div class="col-12 col-sm-12 col-xl-2 col-md-2 col-lg-2">
                        <span id="rating"><strong><i>LinkedIn : @Model.rating</i></strong></span>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12 col-sm-12 col-xl-2 col-md-2 col-lg-2">
                        <span id="star"><strong><i> Github : @Model.star</i></strong></span>
                    </div>
                </div>
            </div>
        </div>
        @*<div class="row mt-5 justify-content-center">
            <div class="col-12 col-xl-2 col-md-2 col-sm-12 col-lg-2 text-center">
                <button class="btn btn-danger" onclick="p()">Save Resume</button>
            </div>
        </div>*@



    </div>
@*</div>*@

